@model IEnumerable<MVCATP.Models.Country>

@{
    ViewBag.Title = "Country";
}

<h2>Countries</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CountryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CountryName)
        </td>

    </tr>
}

</table>
